{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/views/home.html" },
      { "src": "/about", "dest": "/views/about.html" },
      { "src": "/lego/sets", "dest": "/lego/sets" },
      { "src": "/lego/sets/(.*)", "dest": "/lego/sets/$1" },
      { "src": "/(.*)", "dest": "/views/404.html" }
    ]
  }
  